{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "composite": true,
    "module": "preserve",
    "moduleResolution": "bundler",
    "target": "es2020",
    "lib": [
      "es2020"
    ],
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/__tests__/**"
  ],
  "references": [
    {
      "path": "../core"
    }
  ]
}
